{
  "name": "aqiduck",
  "version": "0.1.0",
  "description": "Air quality and temperature aggregator and reporter",
  "main": "dist/index.js",
  "author": "Daniela Wellisz",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@slack/web-api": "^5.11.0",
    "axios": "^0.20.0",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.1",
    "jest": "^26.4.2",
    "ts-jest": "^26.3.0"
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "clean-ts": "rm -rf dist && mkdir dist && tsc",
    "clean-test": "yarn clean-ts && jest",
    "lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx",
    "clean-run": "yarn clean-ts && node .",
    "test-all": "yarn clean-test --coverage && yarn lint && SILENT=true node ."
  }
}
